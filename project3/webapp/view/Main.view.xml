<mvc:View controllerName="project3.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <VBox>
        <Input id="telnoInput" placeholder="XXX-XXXX-XXXX" />
        <Button text="조회" press="onSearch" />
        <List id="resultList" items="{path:'result>/list', templateShareable:false}">
            <StandardListItem title="{result>Matcode}" description="{result>Quant}"/>
        </List>
    </VBox>
    </Page>
</mvc:View>
